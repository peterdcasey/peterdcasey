/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP And2 {
    IN a[2],b[2] ;
    OUT out;

    PARTS:
    // Put your code here:
    And(a=a[0],b=b[0],out=w1);
    And(a=a[1],b=b[1],out=w2);
    And(a=w1, b=w2, out=out);
}
